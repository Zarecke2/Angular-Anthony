{
  "name": "projet",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "e2e": "ng e2e",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "dev:ssr": "ng run Projet:serve-ssr",
    "serve:ssr": "node www/server/main.js",
    "build:ssr": "ng build --configuration production && gzipper compress --level 6 --remove-larger --threshold 900 ./www/app && ng run Projet:server:production && npm run serve:ssr",
    "test": "ng test",
    "bundle-report": "webpack-bundle-analyzer www/app/stats.json"
  },
  "private": true,
